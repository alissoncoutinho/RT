@model Barragem.Models.Barragens
@{
    ViewBag.Title = "Quem Somos";
    Layout = "~/Views/Shared/_LayoutExterno.cshtml";
    HttpCookie cookie = Request.Cookies["_barragemId"];
    var barraId = 0;
    if (cookie != null){
        barraId = Convert.ToInt32(cookie.Value.ToString());
    }
}

<!-- ******Signup Section****** -->
<div class="headline-bg">
</div>
<section class="features-video section section-on-bg">
    <div class="container text-center">
        <h2 class="title">Quem Somos</h2>
    </div><!--//container-->
</section><!--//feature-video-->
<section class="signup-section access-section section">
    <div class="container">
        <div class="row">
            <div class="form-box col-md-12 col-sm-12 col-xs-12">
                <div class="form-box-inner">
                    <h2 class="title text-center"></h2>
                    <p class="intro text-center"></p>
                    <div class="desc text-left">
                        @Html.Raw(Model.quemsomos)
                        <br><br><br>
                    </div><!--//desc-->
                </div><!--//form-box-inner-->
            </div><!--//form-box-->
        </div><!--//row-->
    </div><!--//container-->
</section><!--//signup-section-->
